{
    "name": "buildbot-console-view",
    "main": "buildbot_console_view/static/scripts.js",
    "style": "buildbot_console_view/static/styles.js",
    "scripts": {
        "build": "rimraf buildbot_console_view/static && webpack --bail --progress --profile --env prod",
        "build-dev": "rimraf buildbot_console_view/static && webpack --bail --progress --profile --env dev",
        "test": "karma start",
        "test-watch": "karma start --auto-watch --no-single-run",
        "yarn-update-local": "yarn remove buildbot-data-js guanlecoja-ui && yarn add ../data_module ../guanlecoja-ui"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/plugin-transform-runtime": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@babel/runtime": "^7.4.3",
        "angular-mocks": "1.5.3",
        "autoprefixer": "^9.5.1",
        "babel-loader": "^8.0.5",
        "copy-webpack-plugin": "^5.0.3",
        "css-loader": "^2.1.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "import-glob-loader": "^1.1.0",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jasmine-core": "^3.4.0",
        "karma": "^4.1.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-jasmine": "^2.0.1",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "^0.0.32",
        "karma-webpack": "^3.0.5",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "lodash": "^4.17.11",
        "mini-css-extract-plugin": "^0.6.0",
        "node-libs-browser": "^2.2.0",
        "null-loader": "^1.0.0",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^2.0.0",
        "rimraf": "^2.6.3",
        "style-loader": "^0.23.1",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1",
        "webpack-dev-server": "^3.3.1",
        "webpack-fix-style-only-entries": "^0.2.1"
    },
    "dependencies": {
        "@uirouter/angularjs": "^1.0.15",
        "angular": "1.5.3",
        "angular-animate": "1.5.3",
        "buildbot-data-js": "../data_module",
        "guanlecoja-ui": "../guanlecoja-ui"
